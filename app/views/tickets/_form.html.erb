<%= render 'shared/errors', model_instance: @ticket %>

<%= form_with(model: [ @board, @list, @ticket ], local:true) do |f| %>
    <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name, class: "form-control", placeholder: "Name" %>
    </div>
    <%= f.submit 'Submit' %>
<% end %>